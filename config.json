{
    "settings": {
        "frequency": "15m 30s",
        "rss-port": 8080,
        "browser": "firefox",
        "javascript": true,
        "js-load-time-seconds": 6,
        "messengers": [
            "rss",
            "notify-send",
            "print"
        ],
        "report-first-found": [
            "print"
        ]
    },
    "account-details": {
        "matrix": {
            "homeserver": "https://matrix.org",
            "username": "@:matrix.org",
            "password": "",
            "room-id": "!:matrix.org"
        }
    },
    "monitors": [
        {
            "name": "Thinkpad_t4802",
            "url": "https://www.kleinanzeigen.de/s-thinkpad-t480/k0",
            "url-range": {
                "url-parts": [
                    "https://www.kleinanzeigen.de/s-seite:",
                    "/thinkpad-t480/k0",
                    2
                ],
                "tolerance": 0,
                "max-scrapes": 10
            },
            "url-range-frequency": 15,
            "css-selector": {
                "tag": "div",
                "classes": "aditem-main"
            },
            "text-css-selectors": [
                {
                    "tag": "h2",
                    "classes": "text-module-begin"
                },
                {
                    "tag": "p",
                    "classes": "aditem-main--middle--price-shipping--price"
                }
            ],
            "href-css-selectors": {
                "tag": "h2",
                "classes": "text-module-begin"
            },
            "active": false
        },
        {
            "name": "Thinkpad_t480",
            "url": "https://www.kleinanzeigen.de/s-thinkpad-t480/k0",
            "css-selector": {
                "tag": "div",
                "classes": "aditem-main"
            },
            "filters": [
                {
                    "href-filters": [
                        {
                            "operator": "contains",
                            "filter-value": "TOP",
                            "css-selector": {
                                "tag": "span",
                                "classes": [
                                    "userbadge-tag"
                                ]
                            }
                        },
                        {
                            "operator": "!contains",
                            "filter-value": "Nur Abholung",
                            "css-selector": {
                                "tag": "span",
                                "classes": [
                                    "boxedarticle--details--shipping"
                                ]
                            }
                        },
                        {
                            "css-selector": {
                                "tag": "span",
                                "classes": [
                                    "userprofile-vip"
                                ]
                            },
                            "href-filters": [
                                {
                                    "operator": "!=",
                                    "filter-value": "",
                                    "css-selector": {
                                        "tag": "h2",
                                        "classes": [
                                            "userprofile--name"
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                }
            ],
            "text-css-selectors": [
                {
                    "tag": "h2",
                    "classes": "text-module-begin"
                },
                {
                    "tag": "p",
                    "classes": "aditem-main--middle--price-shipping--price"
                }
            ],
            "href-css-selectors": {
                "tag": "h2",
                "classes": "text-module-begin"
            },
            "active": false
        },
        {
            "name": "Autechre-releases",
            "url": "https://autechre.warp.net/releases",
            "css-selector": {
                "tag": "div",
                "classes": "product-tile"
            },
            "continuous": true,
            "frequency": "1m",
            "filters": [
                {
                    "operator": "!contains",
                    "filter-value": "AE_",
                    "text-css-selector": {
                        "tag": "dd",
                        "classes": "release-title"
                    }
                }
            ],
            "text-css-selectors": {
                "tag": "dd",
                "classes": "release-title"
            },
            "href-css-selectors": {
                "tag": "a",
                "classes": [
                    "main-product-image",
                    "artwork"
                ]
            },
            "href-addition": {
                "tag": "dd",
                "classes": "catalogue-number"
            },
            "active": true
        },
        {
            "name": "bytebytego",
            "comment": "substack offers a rss feed already, just an example.",
            "url": "https://blog.bytebytego.com/archive",
            "css-selector": {
                "tag": "a",
                "classes": [
                    "pencraft",
                    "pc-reset",
                    "color-pub-primary-text-NyXPlw",
                    "font-pub-headings-FE5byy",
                    "clamp-y7pNm8",
                    "clamp-3-lxFDfR",
                    "reset-IxiVJZ"
                ]
            },
            "frequency": "1m",
            "href-addition": {
                "tag": "div",
                "classes": "available-content"
            },
            "active": false
        }
    ]
}
