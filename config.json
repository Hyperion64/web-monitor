{
    "settings": {
        "frequency-minutes": 15,
        "rss-port": 8080,
        "javascript": true,
        "js-load-time-seconds": 6,
        "messengers": [
            "rss",
            "print",
            "notify-send"
        ],
        "report-first-found": [
            "print"
        ]
    },
    "account-details": {
        "matrix": {
            "homeserver": "https://matrix.org",
            "username": "@:matrix.org",
            "password": "",
            "room-id": "!:matrix.org"
        }
    },
    "monitors": [
        {
            "name": "Thinkpad_t480",
            "url": "https://www.kleinanzeigen.de/s-thinkpad-t480/k0",
            "url-range": [
                "https://www.kleinanzeigen.de/s-seite:",
                "/thinkpad-t480/k0",
                2,
                0,
                10
            ],
            "css-selector": {
                "tag": "div",
                "classes": "aditem-main"
            },
            "text-css-selectors": [
                {
                    "tag": "h2",
                    "classes": "text-module-begin"
                },
                {
                    "tag": "p",
                    "classes": "aditem-main--middle--price-shipping--price"
                }
            ],
            "href-css-selectors": {
                "tag": "h2",
                "classes": "text-module-begin"
            },
            "active": true
        },
        {
            "name": "Autechre-releases",
            "url": "https://autechre.warp.net/releases",
            "css-selector": {
                "tag": "div",
                "classes": "product-tile"
            },
            "filters": [
                {
                    "operator": "!contains",
                    "filter-value": "AE_",
                    "text-css-selector": {
                        "tag": "dd",
                        "classes": "release-title"
                    }
                }
            ],
            "text-css-selectors": {
                "tag": "dd",
                "classes": "release-title"
            },
            "href-css-selectors": {
                "tag": "a",
                "classes": [
                    "main-product-image",
                    "artwork"
                ]
            },
            "active": true
        }
    ]
}
